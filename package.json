{
  "name": "event-director",
  "version": "1.2.0",
  "description": "predicate-builder-service",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "start": "node -r dotenv/config build/index.js",
    "clean": "npx rimraf build node_modules coverage package-lock.json",
    "fix": "npm run fix:prettier && npm run fix:eslint",
    "fix:eslint": "eslint --fix",
    "fix:prettier": "prettier --write \"**/*.ts\"",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint",
    "lint:eslint-unused": "eslint --report-unused-disable-directives",
    "lint:eslint-no-inline": "eslint --no-inline-config",
    "lint:prettier": "prettier --check \"**/*.ts\"",
    "prepare": "husky install",
    "test": "jest --config=jest.config.ts --passWithNoTests --forceExit",
    "test:watch": "jest --config=jest.config.ts --watch"
  },
  "keywords": [],
  "contributors": [
    {
      "name": "Johan Foley"
    },
    {
      "name": "Theophilus Okwugwuni"
    },
    {
      "name": "Uche Akogwu"
    },
    {
      "name": "Jean-Pierre Nell"
    }
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@tazama-lf/frms-coe-lib": "6.0.0-rc.3",
    "@tazama-lf/frms-coe-startup-lib": "2.4.0-rc.4",
    "dotenv": "^17.2.0",
    "node-cache": "^5.1.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@stylistic/eslint-plugin": "^5.2.0",
    "@types/async": "^3.2.24",
    "@types/jest": "^30.0.0",
    "@types/node-fetch": "^2.6.12",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "eslint-config-love": "^121.0.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "9.1.7",
    "ioredis-mock": "^8.9.0",
    "jest": "^30.1.2",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "replace": "1.2.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}